<hr>
<%= simple_form_for(habit_list) do |f| %>
  <div class="title">
    <%= f.input :title %>
  </div>

  <div class="description">
    <%= f.input :description %>
  </div>

  <div class="habit_type">
    <%= f.input :habit_type %>
  </div>
  <br>
  <div class="actions">
    <%= f.submit "Submit", class: "btn btn-primary"%>
  </div>
<% end %>
<hr>
